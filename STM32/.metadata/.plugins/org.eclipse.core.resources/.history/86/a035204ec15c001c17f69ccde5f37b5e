#include "com_FPGA_spi.h"


uint8_t txData [2] = {0b10101010, 0b10101010};

void test_spi(){
	moteur1();
	spi_transmit();
	spi_transmit();
	spi_transmit();
	return;
}



void spi_transmit(){
	HAL_SPI_Transmit (&hspi1,txData,2,100);
	return;
}



void moteur1(){
	txData[1] = txData[1] & 0b01111111;
}





